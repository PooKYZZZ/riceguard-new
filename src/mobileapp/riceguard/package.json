{
  "name": "riceguard",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web"
  },
  "dependencies": {
    "@expo-google-fonts/nunito": "^0.4.2",
    "@react-navigation/native": "^7.0.14",
    "@react-navigation/native-stack": "^7.1.7",
    "expo": "~54.0.13",
    "expo-font": "~12.0.9",
    "expo-image-picker": "~15.0.7",
    "expo-status-bar": "~3.0.8",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-safe-area-context": "^4.10.5",
    "react-native-screens": "~4.2.0",
    "react-native-web": "^0.21.0"
  },
  "devDependencies": {
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.4.2",
    "@testing-library/user-event": "^14.5.1",
    "jest": "^29.7.0",
    "react-test-renderer": "19.1.0",
    "jest-expo": "~54.0.1"
  },
  "private": true,
  "jest": {
    "preset": "react-native",
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ],
    "testMatch": [
      "**/__tests__/**/*.test.js",
      "**/*.(test|spec).js"
    ],
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/__tests__/**",
      "!**/coverage/**",
      "!jest.config.js",
      "!jest.setup.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(react-native|@react-native|react-native-*|@react-navigation)/)"
    ]
  }
}
